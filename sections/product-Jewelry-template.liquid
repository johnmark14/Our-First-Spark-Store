<div class="product-template__container page-width"
  id="ProductSection-{{ section.id }}"
  data-section-id="{{ section.id }}"
  data-section-type="product"
  data-enable-history-state="true"
  data-ajax-enabled="{{ settings.enable_ajax }}" 
>
  {% comment %}
    Get first variant, or deep linked one
  {% endcomment %}
  {%- assign current_variant = product.selected_or_first_available_variant -%}
  {%- assign product_image_zoom_size = '1024x1024' -%}
  {%- assign product_image_scale = '2' -%}
  {%- assign enable_image_zoom = section.settings.enable_image_zoom -%}
  {%- assign compare_at_price = current_variant.compare_at_price -%}
  {%- assign price = current_variant.price -%}

  {% case section.settings.media_size %}
    {% when 'small' %}
      {%- assign product_media_width = 'medium-up--one-third' -%}
      {%- assign product_description_width = 'medium-up--two-thirds' -%}
      {%- assign height = 345 -%}
    {% when 'medium' %}
      {%- assign product_media_width = 'medium-up--one-half' -%}
      {%- assign product_description_width = 'medium-up--one-half' -%}
      {%- assign height = 530 -%}
    {% when 'large' %}
      {%- assign product_media_width = 'medium-up--two-thirds' -%}
      {%- assign product_description_width = 'medium-up--one-third' -%}
      {%- assign height = 720 -%}
    {% when 'full' %}
      {%- assign product_media_width = '' -%}
      {%- assign product_description_width = '' -%}
      {%- assign height = 1090 -%}
      {%- assign enable_image_zoom = false -%}
  {% endcase %}

  <div class="grid product-single{% if section.settings.enable_payment_button %} product-single--{{ section.settings.media_size }}-media{% endif %}">
    <div class="grid__item product-single__media-group {{ product_media_width }}{% if section.settings.media_size == 'full' %} product-single__media-group--full{% endif %}" data-product-single-media-group>
      {%- assign featured_media = product.selected_or_first_available_variant.featured_media | default: product.featured_media -%}

      {%- for media in product.media -%}
        {% include 'media', media: media, featured_media: featured_media, height: height, enable_image_zoom: enable_image_zoom, image_zoom_size: product_image_zoom_size, image_scale: product_image_scale %}
      {%- endfor -%}

      <noscript>
        {% capture product_image_size %}{{ height }}x{% endcapture %}
        <img src="{{ featured_media | img_url: product_image_size, scale: product_image_scale }}" alt="{{ featured_media.alt }}" id="FeaturedMedia-{{ section.id }}" class="product-featured-media" style="max-width: {{ height }}px;">
      </noscript>

      {% assign first_3d_model = product.media | where: "media_type", "model" | first %}

      {%- if first_3d_model -%}
        <button
          aria-label="{{ 'products.product.view_in_space_label' | t }}"
          class="product-single__view-in-space"
          data-shopify-xr
          data-shopify-model3d-id="{{ first_3d_model.id }}"
          data-shopify-title="{{ product.title | escape }}"
          data-shopify-xr-hidden
        >
          {% include 'icon-3d-badge-full-color' %}<span class='product-single__view-in-space-text'>{{ 'products.product.view_in_space' | t }}</span>
        </button>
      {%- endif -%}


      {% if product.media.size > 1 %}
        {% if product.media.size > 4 %}
          {%- assign enable_thumbnail_slides = true -%}
        {% endif %}

         <div class="thumbnails-wrapper{% if enable_thumbnail_slides == true %} thumbnails-slider--active{% endif %}">
          {% if enable_thumbnail_slides == true %}
           <!-- <button type="button" class="btn btn--link medium-up--hide thumbnails-slider__btn thumbnails-slider__prev thumbnails-slider__prev--{{ section.id }}"> -->
          <button type="button" class="btn btn--link thumbnails-slider__btn thumbnails-slider__prev thumbnails-slider__prev--{{ section.id }}">
              {% include 'icon-chevron-left' %}
              <span class="icon__fallback-text">{{ 'sections.slideshow.previous_slide' | t }}</span>
            </button>
          {% endif %}
          <ul class="product-single__thumbnails product-single__thumbnails-{{ section.id }}">
            {% for media in product.media %}
             <!-- <li class="product-single__thumbnails-item product-single__thumbnails-item--{{ section.settings.media_size }} js"> -->
            <li class="product-single__thumbnails-item product-single__thumbnails-item--{{ section.settings.media_size }} js no-clear">
                <a href="{{ media.preview_image | img_url: product_image_zoom_size, scale: product_image_scale }}"
                   class="text-link product-single__thumbnail product-single__thumbnail--{{ section.id }}"
                   data-thumbnail-id="{{ section.id }}-{{ media.id }}"
                   {% if enable_image_zoom %}data-zoom="{{ media.preview_image | img_url: product_image_zoom_size, scale: product_image_scale }}"{% endif %}>

                    {%- capture thumbnailAlt -%}
                      {%- if media.media_type == 'video' or media.media_type == 'external_video' -%}
                        {{ 'sections.featured_product.video_thumbnail_alt' | t: imageAlt: media.alt | escape }}
                      {%- elsif media.media_type == 'model' -%}
                        {{ 'sections.featured_product.model_thumbnail_alt' | t: imageAlt: media.alt | escape }}
                      {%- else -%}
                        {{ 'sections.featured_product.gallery_thumbnail_alt' | t: imageAlt: media.alt | escape }}
                      {%- endif -%}
                    {%- endcapture -%}

                    <img class="product-single__thumbnail-image" src="{{ media.preview_image | img_url: '110x110', scale: 2 }}" alt="{{ thumbnailAlt }}">
                    {%- if media.media_type == 'video' or media.media_type =='external_video' -%}
                      <div class="product-single__thumbnail-badge">
                        {% include 'icon-video-badge-full-color' %}
                      </div>
                    {%- endif -%}
                    {%- if media.media_type == 'model' -%}
                      <div class="product-single__thumbnail-badge">
                        {% include 'icon-3d-badge-full-color' %}
                      </div>
                    {%- endif -%}
                </a>
              </li>
            {% endfor %}
          </ul>
          {% if enable_thumbnail_slides == true %}
           <!-- <button type="button" class="btn btn--link medium-up--hide thumbnails-slider__btn thumbnails-slider__next thumbnails-slider__next--{{ section.id }}"> -->
           <button type="button" class="btn btn--link thumbnails-slider__btn thumbnails-slider__next thumbnails-slider__next--{{ section.id }}">
              {% include 'icon-chevron-right' %}
              <span class="icon__fallback-text">{{ 'sections.slideshow.next_slide' | t }}</span>
            </button>
          {% endif %}
        </div>
      {% endif %}
    </div>

    <div class="grid__item {{ product_description_width }}">
      <div class="product-single__meta">

        <h1 class="product-single__title">{{ product.title }}</h1>
        {% comment %}Start automatically added Judge.me widget{% endcomment %}
          {% render 'judgeme_widgets', widget_type: 'judgeme_preview_badge', concierge_install: true, product: product %}
        {% comment %}End automatically added Judge.me widget{% endcomment %}
        
        <!-- MOd change snippets used : PVA39 John Mark  -->
        <div class="product__price">
          {% include 'product-page-price', variant: current_variant, show_vendor: section.settings.show_vendor %}
        </div>
  
          {%- if shop.taxes_included or shop.shipping_policy.body != blank -%}
            <div class="product__policies rte" data-product-policies>
              {%- if shop.taxes_included -%}
                {{ 'products.product.include_taxes' | t }}
              {%- endif -%}
              {%- if shop.shipping_policy.body != blank -%}
                {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
              {%- endif -%}
            </div>
          {%- endif -%}

          {% capture "form_classes" -%}
            product-form product-form-{{ section.id }}
            {%- unless section.settings.show_variant_labels %} product-form--hide-variant-labels {% endunless %}
            {%- if section.settings.enable_payment_button and product.has_only_default_variant %} product-form--payment-button-no-variants {%- endif -%}
            {%- if current_variant.available == false %} product-form--variant-sold-out {%- endif -%}
          {%- endcapture %}

          <!--###################### START Custom Similar Products Author: PVA39 JOHN MARK  ###################### -->

          {% assign product_limit = 5 %}
          {% assign score = 2 %}
          {% assign image_size = 'compact' %}
          {% assign all_collection = 'related' %}
          {% assign heading = 'Similar Products:' %}

          {% for c in product.collections %}
            {% if c.handle == all_collection %}
              {% assign collection = c %}
              
            {% endif %}
          {% endfor %}

          {% if collection and collection.products_count > 1 %}
           
            {% assign i = 0 %}
            {% assign current_product = product %}
            {% assign current_product_tags = current_product.tags | join: ' ' %}

            <div class="similar-products s-m-top s-p-right s-p-left">
              <div class="similar-products__inner-container display-flex align-items-c justify-content-space-between">
                <div id="xyzheading" class="similar-products__label">
                  <span><b>{{heading}}</b></span>
                </div>
                <div class="similar-products__container display-flex align-items-c justify-content-c">
                  {% for product in collection.products %}

                    {% assign s = 0 %}
                    {% for tag in product.tags %}
                      {% if current_product_tags contains tag %}
                        {% assign s = s | plus: 1 %}
                      {% endif %}
                    {% endfor %}

                    {% if s > score and i < product_limit and current_product.handle != product.handle %}
                      {% assign i = i | plus: 1 %}
                      <div class="similar-products__container--img">
                        <a href="{{product.url | within: collection }}"><img src="{{product.featured_image.src | img_url: 'medium'}}"></a>
                      </div>
                    {% endif %}

                  {% endfor %}
                  <div class="similar-products__container--option">
                    <a class="similar-products__link" href="#"><span>...</span></a>
                  </div>
                </div>
              </div>           
            </div>
          {% endif %}

          <!-- Files included 
            @ Snippets folder : similar-products-modal.liquid
            reference to theme.liquid line 139
            -->
          <!--###################### END Custom Similar Products Author: PVA39 JOHN MARK  ###################### -->
          
          <!--###################### START Custom Form Author: PVA39 JOHN MARK  ###################### -->
          {% form 'product', product, class:form_classes, data-product-form: '', autocomplete: 'off' %}
            {% unless product.has_only_default_variant %}
                <div class="product-form__controls-group custom-form__controls-group" style="display: none;">
                  {% for option in product.options_with_values %} 
                    <div class="selector-wrapper js product-form__item">
                      <label {% if option.name == 'default' %}class="label--hidden" {% endif %}for="SingleOptionSelector-{{ forloop.index0 }}" data-option-name="{{ option.name }}">
                        {{ option.name }}
                      </label>
                      <select class="single-option-selector single-option-selector-{{ section.id }} product-form__input"
                        id="SingleOptionSelector-{{ forloop.index0 }}" 
                        data-index="option{{ forloop.index }}"
                      >
                        {% for value in option.values %}
                          <option value="{{ value | escape }}"{% if option.selected_value == value %} selected="selected"{% endif %}>{{ value }}</option>
                        {% endfor %}
                      </select>
                    </div>
                  {% endfor %}
                </div>

              <div class="product-form__controls-group custom-form__controls-group">
                <div id="radio-options" class="selector-wrapper js product-form__item">
                  {% for option in product.options_with_values %}
                    <div class="form-group" data-option-id="SingleOptionSelector-{{forloop.index0}}">
                      <label {% if option.name == 'default' %}class="label--hidden" {% endif %}>
                          <span class="option-title ">{{ option.name }}: &nbsp;</span>
                          <span class="option-value">{{option.selected_value}}</span>
                      </label>
                      <div class="single-option-selector">
                        {% for value in option.values %}
                          <input type="radio" name="{{ option.name }}" value="{{ value | escape }}" class="option-input" id="{{ option.name }}{{forloop.index}}" {% if option.selected_value == value %} checked {% endif %}>
                          <label for="{{ option.name }}{{forloop.index}}" class="option-input-label">{{ value }}</label>
                        {% endfor %}  
                      </div>
                    </div>
                    {% endfor %}
                </div>
              </div>

              <!--###################### START Photo Preview Effect Author: PVA39 JOHN MARK  ###################### -->
              <div class="product-form__preview-text m-m-top">
                {% assign photo = 'Photo Necklace' %}
                {% for tag in product.tags %}
                  {% if tag contains photo %}
                    <!-- Photo frame can be added in Metafields -->
                    <div id="photo-frame" data-photo-frame="{{ product.metafields.photo-frame.frame }}"></div>
                    <div class="selector-wrapper js product-form__item">
                      <div id="upload-photo-1" class="upload-photo">
                        <div class="upload-photo__inner"> 
                          <input type="file" name="properties[Original Photo]" id="photo-file" accept=".jpg, .png, .jpeg" class="photo-input" required="required">
                          <span class="add-img">
                            <span>Upload A Picture</span>
                            <i class="fa fa-upload"></i> 
                          </span>
                        </div>
                      </div>
                      <input id="edited-photo" name="properties[_Edited Photo]" type="text" style="display: none;">
                      <div id="upload-photo-2" class="upload-photo after" style="display: none;">
                        <div class="big">
                          <img name="properties[After Photo]" alt="" id="preview-crop" width="50" height="40">
                          <i class="fa fa-search-plus"></i>
                        </div>
                        <div class="after-preview">
                          <span>Change Picture</span>
                          <input type="file" id="photo-file-2" accept=".jpg, .png, .jpeg" class="photo-input">
                        </div>
                      </div>
                    </div>
                  {% endif %}
                {% endfor %}
                <!--###################### START Engraving text Preview Effect Author: PVA39 JOHN MARK  ###################### -->
                {% for tag in product.tags %}
                  {% if tag contains 'Engrave Necklace' %}
                  <div id="engrave-frame" data-engrave-text-config='{{ product.metafields.engrave-text-config.config }}'></div>
                    <div class="selector-wrapper js product-form__item">
                      <div class="engraving-container">
                        <div id="preview-engraving-text" class="big">
                          <img id="engrave-preview-crop" alt="" id="preview-engraving-crop" width="50" height="40">
                          <i class="fa fa-search-plus"></i>
                        </div>
                        <div id="preview-engraving-letter" class="preview-lettering">
                          <span class="preview-lettering__title">
                            Add Engravings
                          </span>
                        </div>
                        <div class="after-preview-text">
                          <span class="add-price" style="display: none;">$3.99</span>
                          <button type="button" id="add-engraving">Add</button> 
                        </div> 
                      </div>
                      <div class="engraving-options" style="display: none;">
                        <input id="engraving-text" type="checkbox" name="properties[Text]" value="None" required>
                        <input type="text" name="properties[Font Style]" id="engraving-font">
                        <input id="engraving-photo" type="checkbox" name="properties[_Engraving Photo]" value="None" required>
                      </div>
                    </div>
                  {% endif %}
                {% endfor %} 

                <div class="selector-wrapper js product-form__item">
                  <!-- ------------------------Variabbles---------------------------------- -->
                  
                  {%- comment -%}
                    Liquid variable for max-input
                    Input Text Max Inputs
                  {%- endcomment -%}
                  <!-- Product Tags -->
                  {% assign tag_1_name = '1 Name' %}
                  {% assign tag_2_name = '2 Name' %}
                  {% assign tag_3_name = '3 Name' %}
                  {% assign tag_4_name = '4 Name' %}
                  <!-- Note* There's also a way to add a max length for an specific product using Metatfields
                            Here's the format:
                                    Value type: Integer
                                    Namespace: input-max
                                    key: max-1
                                      note* max-1 for input_1_max
                                            max-2 for input_2_max
                                            max-3 for input_3_max
                                            max-4 for input_4_max
                            See this two products for Metafields example: Personalized Name Necklace & Customized 3 Names
                  -->
                  <!-- Product input Max -->
                  <!-- Default values -->
                  {% assign input_1_max = 13 %}
                  {% assign input_2_max = 13 %}
                  {% assign input_3_max = 13 %}
                  {% assign input_4_max = 13 %}

                  {% for text_limit in product.tags %}
                    <!-- ####################################### 1 Name ################################################### -->
                    {% if text_limit contains tag_1_name %}
                      <!-- Input_1_max -->
                      {% if product.metafields.input-max['max-1'] != blank %}
                        {% assign input_1_max = product.metafields.input-max['max-1'] %}
                      {% else %}
                        <!-- Change my value here -->
                        {% assign input_1_max = 13 %}
                      {% endif %}
                    {% endif %} 
                    <!-- ####################################### 2 Name ################################################### -->
                    {% if text_limit contains tag_2_name %}
                      <!-- Input_1_max -->
                      {% if product.metafields.input-max['max-1'] != blank %}
                        {% assign input_1_max = product.metafields.input-max['max-1'] %}
                      {% else %}
                        <!-- Change my value here -->
                        {% assign input_1_max = 8 %}
                      {% endif %}
                      <!-- Input_2_max -->
                      {% if product.metafields.input-max['max-2'] != blank %}
                        {% assign input_2_max = product.metafields.input-max['max-2'] %}
                      {% else %}
                        <!-- Change my value here -->
                        {% assign input_2_max = 8 %}
                      {% endif %}
                    {% endif %}
                    <!-- ####################################### 3 Name ################################################### -->
                    {% if text_limit contains tag_3_name %}
                      <!-- Input_1_max -->
                      {% if product.metafields.input-max['max-1'] != blank %}
                        {% assign input_1_max = product.metafields.input-max['max-1'] %}
                      {% else %}
                        <!-- Change my value here -->
                        {% assign input_1_max = 9 %}
                      {% endif %}
                      <!-- Input_2_max -->
                      {% if product.metafields.input-max['max-2'] != blank %}
                        {% assign input_2_max = product.metafields.input-max['max-2'] %}
                      {% else %}
                        <!-- Change my value here -->
                        {% assign input_2_max = 8 %}
                      {% endif %}
                      <!-- Input_3_max -->
                      {% if product.metafields.input-max['max-3'] != blank %}
                        {% assign input_3_max = product.metafields.input-max['max-3'] %}
                      {% else %}
                        <!-- Change my value here -->
                        {% assign input_3_max = 8 %}
                      {% endif %}
                    {% endif %}
                    <!-- ####################################### 4 Name ################################################### -->
                    {% if text_limit contains tag_4_name %}
                      <!-- Input_1_max -->
                      {% if product.metafields.input-max['max-1'] != blank %}
                        {% assign input_1_max = product.metafields.input-max['max-1'] %}
                      {% else %}
                        <!-- Change my value here -->
                        {% assign input_1_max = 8 %}
                      {% endif %}
                      <!-- Input_2_max -->
                      {% if product.metafields.input-max['max-2'] != blank %}
                        {% assign input_2_max = product.metafields.input-max['max-2'] %}
                      {% else %}
                        <!-- Change my value here -->
                        {% assign input_2_max = 8 %}
                      {% endif %}
                      <!-- Input_3_max -->
                      {% if product.metafields.input-max['max-3'] != blank %}
                        {% assign input_3_max = product.metafields.input-max['max-3'] %}
                      {% else %}
                        <!-- Change my value here -->
                        {% assign input_3_max = 8 %}
                      {% endif %}
                      <!-- Input_3_max -->
                      {% if product.metafields.input-max['max-4'] != blank %}
                        {% assign input_4_max = product.metafields.input-max['max-4'] %}
                      {% else %}
                        <!-- Change my value here -->
                        {% assign input_4_max = 8 %}
                      {% endif %}
                    {% endif %}
                  {% endfor %}
                  <!-- ------------------------END Variabbles---------------------------------- -->
                  {% for tag in product.tags %}
                    {% if tag contains tag_3_name or tag contains tag_4_name %}
                      <div id="number-text" data-number-text="{%- if tag contains tag_3_name -%}{{-tag_3_name-}}{%- else -%}{{-tag_4_name-}}{%- endif -%}">
                        <input id="text-capture" name="properties[_Edited Photo]" type="text" style="display: none;">
                      </div>
                      <div class="text-preview-container">
                        <div id="product-form__input" class="form-input-1 product-form r-p-all display-grid grid-col-1" data-tag-max="{{input_2_max}},{{input_3_max}},{{input_4_max}}">
                          <div id="container--input-1" class="product-form__container--input">
                            <input class="product-form__container--input-text product-form__input s-m-top " type="text" name="properties[Custom Text 1]"
                              id="text-preview-input-1" placeholder="Enter name 1" maxlength="{{input_1_max}}" minlength="2" required="required">
                            <label class="product-form__container--input-label" for="text-preview-input-1" id="input-preview-label-1"><span>{{input_1_max}}</span> Left</label>
                          </div>
                        </div> 
  
                        <div class="product-form r-p-all form-label-1"> 
                          <div class="product-form__container display-flex"> 
                            <label class="product-form__container--label s-m-right" for="text-preview-input-1">Preview:</label>
                            <div id="container-inner-label" class="product-form__container--inner display-grid grid-col-1" data-font-family="{{product.metafields.fonts['font-1']}}">
                              <label class="product-form__container--text font-family-{{product.metafields.fonts['font-1']}} font-size-l font-color-gold" type="text" 
                              name="text-preview" id="text-preview-1"></label>
                            </div>
                          </div>
                        </div>
                      </div> 
                    {% elsif tag contains tag_2_name %}
                      <div id="number-text" data-number-text="{{tag_2_name}}">
                        <input id="text-capture" name="properties[_Edited Photo]" type="text" style="display: none;">
                      </div>

                      <div class="product-form r-p-all">
                        <div class="product-form__container display-flex">
                          <label class="product-form__container--label s-m-right" for="text-preview-input-1">Preview:</label>
                          <label class="product-form__container--text font-family-{{product.metafields.fonts['font-1']}} font-size-l font-color-gold" 
                            name="text-preview" id="text-preview-1"></label>
                          <label class="product-form__container--text font-family-{{product.metafields.fonts['font-1']}} font-size-l font-color-gold" 
                            name="text-preview" id="text-preview-2"></label>
                        </div>
                      </div>
                     
                      <div class="product-form r-p-all display-grid grid-col-2">
                        <div class="product-form__container--input">
                          <input class="product-form__container--input-text product-form__input s-m-top " type="text" name="properties[Custom Text 1]" 
                            id="text-preview-input-1" placeholder="Enter name 1" maxlength="{{input_1_max}}" minlength="2" required="required">
                          <label class="product-form__container--input-label" for="text-preview-input-1" id="input-preview-label-1"><span>{{input_1_max}}</span> Left</label>
                        </div>
                        <div class="product-form__container--input">
                          <input class="product-form__container--input-text product-form__input s-m-top " type="text" name="properties[Custom Text 2]" 
                            id="text-preview-input-2" placeholder="Enter name 2" maxlength="{{input_2_max}}" minlength="2" required="required">
                          <label class="product-form__container--input-label" for="text-preview-input-2" id="input-preview-label-2"><span>{{input_2_max}}</span> Left</label>
                        </div>
                      </div> 
                      {% break %}
                    {% elsif tag contains tag_1_name %}
                      <div id="number-text" data-number-text="{{tag_1_name}}">
                        <input id="text-capture" name="properties[_Edited Photo]" type="text" style="display: none;">
                      </div>

                      <div class="product-form__container display-flex">
                        <label class="product-form__container--label s-m-right" for="text-preview-input">Preview:</label>
                        <label class="product-form__container--text font-family-{{product.metafields.fonts['font-1']}} font-size-l font-color-gold" 
                          name="text-preview" id="text-preview"></label>
                      </div>
                      <div class="product-form__container--input display-flex">
                        <input class="product-form__container--input-text product-form__input s-m-top " type="text" name="properties[Custom Text]" 
                          id="text-preview-input" placeholder="Enter name or text here" minlength="2" maxlength="{{input_1_max}}" required="required">
                        <label class="product-form__container--input-label" for="text-preview-input" id="input-preview-label"><span>{{input_1_max}}</span> Left</label>
                      </div>
                      {%break%}
                    {% endif %}
                  {% endfor %}
                </div>
                {% assign font-styles = 'Font Styles' %}
                {% for font-tag in product.tags %}
                  {% if font-tag contains font-styles %}
                    <div class="selector-wrapper js product-form__item">
                      <div class="text-preview-fontstyle">
                        <label for="font-style">Font Styles:</label>
                        <div class="product-form__container--grid s-m-top">
                          {% if product.metafields.fonts != blank  %}
                            {% for font in product.metafields.fonts %} 
                              {% assign font_name = font | last | replace: "-", " " | capitalize %}
                              {% assign font_id =  font | first%} 
                              {% assign font_value =  font | last%} 
                              <input class="product-form__radio" type="radio" name="properties[Font Style]" id="text-{{font_id}}" 
                                data-font-family="font-family-{{font_value}}" value="{{font_name}}" {% if font_id == 'font-1'%}checked{% endif %}> 
                              <label class="product-form__font-style font-family-{{font_value}}" for="text-{{font_id}}">Abc</label>
