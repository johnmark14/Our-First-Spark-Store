              <div class="selector-wrapper js product-form__item">
                {% for tag in product.tags %} 
                    {% if tag contains "Gift box" %}
                      {% include 'product-giftbox' %}
                    {% endif %}
                {% endfor%}
                {% for tag in product.tags %} 
                    {% if tag contains "VIP service" %}
                      {% include 'product-vip-service' %}
                    {% endif %}
                {% endfor%} 
              </div>
              <!--###################### End Other Services Author: PVA39 JOHN MARK  ###################### -->
